      <sectiondef kind="define">
      <memberdef kind="define" id="scalar__multiplication_8hpp_1aa1e74ac9f6e034680f02523fb962bd0c" prot="public" static="no">
        <name>GLM_IMPLEMENT_SCAL_MULT</name>
        <param><defname>Vec</defname></param>
        <initializer>	template&lt;typename T&gt; \
	return_type_scalar_multiplication&lt;T, Vec&gt; \
	operator*(T const&amp; s, Vec rh){ \
		return rh *= static_cast&lt;float&gt;(s); \
	} \
	 \
	template&lt;typename T&gt; \
	return_type_scalar_multiplication&lt;T, Vec&gt; \
	operator*(Vec lh, T const&amp; s){ \
		return lh *= static_cast&lt;float&gt;(s); \
	} \
	 \
	template&lt;typename T&gt; \
	return_type_scalar_multiplication&lt;T, Vec&gt; \
	operator/(Vec lh, T const&amp; s){ \
		return lh *= 1.0f / s; \
	}</initializer>
