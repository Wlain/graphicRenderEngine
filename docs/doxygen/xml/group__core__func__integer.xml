        <definition>GLM_FUNC_DECL vec&lt; L, T, Q &gt; glm::bitfieldExtract</definition>
        <argsstring>(vec&lt; L, T, Q &gt; const &amp;Value, int Offset, int Bits)</argsstring>
        <name>bitfieldExtract</name>
        <param>
          <type><ref refid="structvec" kindref="compound">vec</ref>&lt; L, T, Q &gt; const &amp;</type>
